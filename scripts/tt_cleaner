for X in Windows.old*/ "Restored from Carbonite" Backup* "My Backup*"
	do if [ -d "$X" ]
		then
		test $MSG -gt 1 && echo -e "\n${CPUR}Running in subfolder $CWHT$X$COFF\n"
		CWD="$(pwd)"
		cd "$X"
		if echo "$0" | grep -q '^/'
			then $0 $@ --no-report
			else "$CWD/$0" $@
		fi
		cd "$CWD"
		unset CWD
	fi
done

# Push cleaning action to TSS if possible
A_CLEANER=1
test "$TSS_CLIENT" = "1" && test "$TSS_CLIENT_DISABLE" != "1" && \
	test "$NOREPORT" != "1" && while true
	do
	if push_actions
		then test $MSG -gt 0 && echo "Cleaning reported to TSS successfully."
		break
	fi
	echo "Could not push cleaning report. Check your cables and network connection."
	echo "Press 'x' to abort or any other key to retry."
	read -n 1 -s X
	test "$X" = "x" && echo "Aborting cleaning report push. Use 'tss_push_all' to try again later." && break
done

