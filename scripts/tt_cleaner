# Load file list in a background process
if [ "$NOFILESCAN" = "1" ]
	then test $MSG -gt 1 && echo -e "\n${CYEL}Skipping file scans (${CWHT}-N${CYEL} specified)$COFF"
	else
	test $MSG -gt 1 && echo -e "\nLoading file list. Please wait."
	find . 2>/dev/null >$TMPFILE

	# Scan files
	NAME='*.crdownload'; DESC="Partial downloads"; SP='\.'; add_to_scan
	NAME='*.partial'; DESC="Partial downloads"; SP='\.'; add_to_scan
	NAME='Thumbs\.db'; DESC="Thumbnail DBs"; SP='\.'; add_to_scan
	NAME='ehthumbs_vista.db'; DESC="Thumbnail DBs"; SP='\.'; add_to_scan
	NAME='\.thumbnails'; DESC="Thumbnails"; SP="\./$P"; add_to_scan
	NAME='\.picasa.ini'; DESC="Picasa INIs"; SP="\./$P"; add_to_scan
	NAME='ZbThumbnail.info'; DESC="ZoomBrowser thumbs"; SP="\./$P"; add_to_scan
	NAME='hs_err_pid.*\.log'; DESC="hs_err_pid logs"; SP="\./$P"; add_to_scan
	NAME='\~\$[^/]*\.[^/]*'; DESC="Office temp files"; SP='\.'; add_to_scan
	NAME='\~WRL[^/]*\.tmp'; DESC="Office temp files"; SP='\.'; add_to_scan
	NAME='\~uTorrentPartFile_.*.dat'; DESC="uTorrent part file"; SP='\.'; add_to_scan
	# Windows folder viruses
	for X in \
		sdra64.exe winlogon86.exe \
		'TDSS?????*' 'UAC??????*' 'VOID?????*' \
		'_VOID?????*' 'PRAGMA???*' 'pragma???*' \
		exe.exe svchast.exe nail.exe \
		winlgoon.exe iesafemode.exe
		do NAME="${X}"; DESC="KNOWN VIRUS"; SP='\.'; add_to_scan
	done
	# AppData viruses
	for X in \
	av.exe ave.exe hotfix.exe virus.exe
		do NAME="${X}"; DESC="KNOWN VIRUS"; SP="\./$P"; add_to_scan
	done
	rmdir --ignore-fail-on-non-empty "$P/$ROAM/$STARTMENU/*/" "$PD/$STARTMENU/*/" 2>/dev/null
fi

