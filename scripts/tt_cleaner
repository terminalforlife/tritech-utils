#!/bin/bash

VER="5.10.1"
VERDATE="2022-09-02"

# Tritech Service System cleaning tool for Linux
# Copyright (C) by Jody Bruchon <jody@jodybruchon.com>

# Must be run from the root of the filesystem to clean.

. tss__common
test -z "$TSS_COMMON" && echo "Library 'tss__common' failed to load, aborting." && exit 1
. tss_client
test -z "$TSS_CLIENT_VER" && \
	echo "${CYEL}NOTICE: ${CWHT}TSS client missing, can't record cleaning action$COFF"

IFS0="$IFS"
IFS1=$(echo -en "\n\b")
IFS="$IFS1"
CLEANED=0
RW=0
FB_ADD=0
LASTTYPE="none"
TMPFILE=$(mktemp)
shopt -s nullglob
shopt -s nocaseglob
shopt -s nocasematch

control_c () {
	rm -f "$TMPFILE"
	test $MSG -gt 0 && echo -e "\nAborted by user. Cleaned $CLEANED KB."
	exit 1
}

trap control_c INT

# Initialize some settings
NODEL=0; MSG=2; DEBUG=0; NOSIZE=0; NOFILESCAN=0
unset FORCED NOREPORT

# Parse command line parameters
for X in $@
	do if [[ "$X" = "-h" || "$X" = "--help" ]]
		then
		PN="$(basename $0)"
		echo -e "Tritech Service System Cleaner, a junk cleaning tool for Linux\n"
		echo "Program version $VER, last updated on $VERDATE"
		echo "$PN         normal mode, shows verbose scan progress and reports"
		echo "$PN -x      fast normal mode, skips size counts of cleaned items"
		echo "$PN -q      quiet mode, with progress dots and a summary"
		echo "$PN -Q      fast quiet mode, with progress but no summary"
		echo "$PN -s      silent mode, no output, returns an exit code"
		echo "$PN -n      normal mode, but don't actually delete anything"
		echo "$PN -d      debug mode: -n, plus shows what would be deleted"
		echo "$PN -N      do not run the time-consuming file list scan"
		echo "$PN --no-report  do not report cleaning to TSS"
		echo "$PN --[os]  Force OS type: --xp --v7 --osx"
		echo "$PN -h      this help screen"
		echo "$PN --help  this help screen with four wasted keystrokes"
		echo -e "\nCopyright (C) Jody Bruchon <jody@jodybruchon.com>"
		exit 0
	fi
	test "$X" = "-v" && echo "Tritech Service System Cleaner $VER ($VERDATE)" && exit 0
	# Change parameters for different command switches
	# MSG: 0=none, 1=quiet, 2=full
	# NODEL: 0=delete, 1=no delete
	# NOSIZE: 0=get size of cleaned items, 1=skip calculation (faster)
	test "$X" = "-s" && MSG=0 && NOSIZE=1
	test "$X" = "-q" && MSG=1
	test "$X" = "-Q" && MSG=1 && NOSIZE=1
	test "$X" = "-x" && MSG=2 && NOSIZE=1
	test "$X" = "-n" && MSG=2 && NODEL=1
	test "$X" = "-d" && MSG=2 && NODEL=1 && DEBUG=1
	test "$X" = "-dd" && MSG=2 && NODEL=1 && DEBUG=2
	test "$X" = "-N" && NOFILESCAN=1
	# Allow forcing the OS selection and directories
	test "$X" = "--xp" && export V7=1 && FORCED=1
	test "$X" = "--v7" && export V7=2 && FORCED=1
	test "$X" = "--osx" && export OSX=1 && FORCED=1
	test "$X" = "--no-report" && NOREPORT=1
done

### Scan for 2000/XP or Vista/7 or Mac OS X and set folders accordingly
export V7=0
export OSX=0
export UPF=0
export LOCAL=__NONE__
export ROAM=__NONE__
export LIBRARY=__NONE__
OSYS="None"

# Skip OS structure checks if forced
if [ -z "$FORCED" ]
	then
	test -d "$XPDOCS" && export V7=1 && export UPF=1
	if [ -d "$V7DOCS" ]
		then export V7=2 && export UPF=1
		# XPREDO is for if a non-symlink XPDOCS exists
		test -e "$XPDOCS" && test ! -h "$XPDOCS" && XPREDO=1
	fi
	if [[ -d "System/Library" || -d "Users/Shared/Library" ]]
		then export OSX=1 && export V7=0
		test -d "Users/Shared/Library" && export UPF=1
	fi
fi

if [ "$V7" = "1" ]
	then
	OSYS="Windows 2000/XP"
	export P="$XPDOCS/*"
	export LOCAL="$P/$XPLOCAL"
	export ROAM="$P/$XPROAM"
fi
if [ "$V7" = "2" ]
	then
	OSYS="Windows Vista/7/8.x/10"
	export P="$V7DOCS/*"
	export LOCAL="$P/$V7LOCAL"
	export ROAM="$P/$V7ROAM"
fi
if [ "$OSX" = "1" ]
	then
	OSYS="Mac OS X"
	export P="Users/*"
	export LIBRARY="$P/Library"
fi
test "$FORCED" = "1" && OSYS="$OSYS (forced)"

# Scan version of P for grep-friendliness
SCANP="$(echo "$P" | sed 's#\*#\[\^/\]\*#g')"
### End OS version scan

### Common variables and locations

APPLE="Apple Computer"
CARBONITEUS="Carbonite Restored OLD User Settings"
JAVACACHE="Sun/Java/Deployment/cache"
JSYSCACHE="Sun/Java/Deployment/SystemCache"
MBAMLOGS="Malwarebytes/Malwarebytes\' Anti-Malware/Logs"
MBAMQUAR="Malwarebytes/Malwarebytes\' Anti-Malware/Quarantine"
MOZCNTRL="MozillaControl/profiles/MozillaControl/*"
MSSQLLOG="$MS SQL Server/*/Setup Bootstrap/LOG"
